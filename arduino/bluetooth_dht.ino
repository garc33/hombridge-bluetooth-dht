#include <SoftwareSerial.h>#include <DHT.h>#define DELAY 2000 //sleep 2s#define DHT_TYPE DHT22#define DHT_PIN 9#define RX_PIN 10#define TX_PIN 11DHT dht = DHT(DHT_PIN, DHT_TYPE);SoftwareSerial bt = SoftwareSerial(RX_PIN, TX_PIN);void setup() {    bt.begin(9600);    dht.begin();}void loop() {    delay(DELAY);        float temperature = dht.readTemperature();    float humidity = dht.readHumidity();    if(isnan(temperature) || isnan(humidity)) {      // cannot read data from sensor      return;    }    String json = "{\"temperature\":" + String(temperature) + ",\"humidity\":" + String(humidity) + "}";    bt.println(json);}